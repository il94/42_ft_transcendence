{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cpFolders": "cp -r uploads ./dist && cp -r defaultChannelAvatar ./dist && cp -r defaultUserAvatars ./dist",
    "setDatabase": "npx prisma generate && npx prisma migrate deploy &",
    "deploy": "npm install && npm run setDatabase && npm run build && npm run cpFolders && npm run start:prod"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/platform-socket.io": "^10.4.13",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/websockets": "^10.2.10",
    "@prisma/client": "^5.5.2",
    "argon2": "^0.31.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-session": "^1.17.3",
    "file-type-mime": "^0.3.8",
    "fs": "^0.0.1-security",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-42": "^1.2.6",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.3",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
